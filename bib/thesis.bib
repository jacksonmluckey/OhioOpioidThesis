  title = {Racial Discrimination in Medical Care Settings and Opioid Pain Reliever Misuse in a {{U}}.{{S}}. Cohort: 1992 to 2015},
  shorttitle = {Racial Discrimination in Medical Care Settings and Opioid Pain Reliever Misuse in a {{U}}.{{S}}. Cohort},
  author = {Samuel L. Swift and M. Maria Glymour and Tali Elfassy and Cora Lewis and Catarina I. Kiefe and Stephen Sidney and Sebastian Calonico and Daniel Feaster and Zinzi Bailey and Adina {Zeki Al Hazzouri}},
