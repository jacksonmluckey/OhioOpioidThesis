@article{ciceroIncreasedUseHeroin2017,
  title = {Increased Use of Heroin as an Initiating Opioid of Abuse},
  author = {Cicero, Theodore J. and Ellis, Matthew S. and Kasper, Zachary A.},
  year = {2017},
  month = nov,
  volume = {74},
  pages = {63--66},
  issn = {0306-4603},
  doi = {10.1016/j.addbeh.2017.05.030},
  abstract = {Introduction
Given the relatively recent growth in access to heroin and a more permissive atmosphere surrounding its use, we hypothesized that an increasing number of persons with limited experience and tolerance to opioids would experiment with heroin as their first opioid rather than more common prescription opioid analgesics.
Methods
Individuals entering substance abuse treatment for an opioid use disorder in the period 2010\textendash{}2016 (N=5885) were asked about the specific opioid they first regularly used to get high. To limit long-term recall and survival bias, analyses was restricted to opioid initiation that occurred in the past ten years (2005\textendash{}2015).
Results
In 2005, only 8.7\% of opioid initiators started with heroin, but this sharply increased to 33.3\% (p{$<$}0.001) in 2015, with no evidence of stabilization. The use of commonly prescribed opioids, oxycodone and hydrocodone, dropped from 42.4\% and 42.3\% of opioid initiators, respectively, to 24.1\% and 27.8\% in 2015, such that heroin as an initiating opioid was now more frequently endorsed than prescription opioid analgesics.
Conclusions
Our data document that, as the most commonly prescribed opioids \textendash{} hydrocodone and oxycodone \textendash{} became less accessible due to supply-side interventions, the use of heroin as an initiating opioid has grown at an alarming rate. Given that opioid novices have limited tolerance to opioids, a slight imprecision in dosing inherent in heroin use is likely to be an important factor contributing to the growth in heroin-related over dose fatalities in recent years.},
  file = {C\:\\Users\\mollymute\\Zotero\\storage\\GAGJKW8T\\Cicero et al. - 2017 - Increased use of heroin as an initiating opioid of.pdf;C\:\\Users\\mollymute\\Zotero\\storage\\DUSSEJUS\\S0306460317302083.html},
  journal = {Addictive Behaviors},
  keywords = {Heroin,Heroin overdose,Opioid abuse,Opioid initiation,Prescription opioid abuse},
  language = {en}
}

