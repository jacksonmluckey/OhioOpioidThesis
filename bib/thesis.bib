@article{marshallEpidemiologyFentanylinvolvedDrug2017,
  title = {Epidemiology of Fentanyl-Involved Drug Overdose Deaths: A Geospatial Retrospective Study in {{Rhode Island}}, {{USA}}},
  shorttitle = {Epidemiology of Fentanyl-Involved Drug Overdose Deaths},
  author = {Marshall, Brandon D. L. and Krieger, Maxwell S. and Yedinak, Jesse L. and Ogera, Patricia and Banerjee, Priya and {Alexander-Scott}, Nicole E. and Rich, Josiah D. and Green, Traci C.},
  year = {2017},
  volume = {46},
  pages = {130--135},
  issn = {0955-3959},
  doi = {10.1016/j.drugpo.2017.05.029},
  abstract = {BACKGROUND: We conducted an epidemiological investigation of fentanyl and non-fentanyl overdose deaths in Rhode Island to inform overdose prevention efforts. {$<$}br{$><$}br{$>$}METHODS: All drug overdose deaths occurring in Rhode Island between January 1, 2014 and September 30, 2016 were included. Overdose circumstances and decedent characteristics were ascertained from medical examiner files. Toxicological reports provided quantification of fentanyl and metabolites. ArcGIS was used to map exact overdose locations. We compared fentanyl-associated with non-fentanyl overdose deaths using chi-square tests and Wilcoxon rank sum tests. To identify overdose clusters, we used the Getis-Ord Gi* statistic. {$<$}br{$><$}br{$>$}RESULTS: A total of 778 overdose deaths were observed during the study period. In total, 358 (46.0\%) were attributable to acute fentanyl intoxication, increasing from 84 (35.0\%) in 2014 to 138 (55.6\%) during the first nine months of 2016 (Mantel-Haenszel test for trend P{$<$}0.001). Compared to non-fentanyl overdoses, fentanyl overdose decedents were younger, more likely to have multiple drugs contribute to the death, and have evidence of injection drug use. The geospatial distribution of fentanyl and non-fentanyl involved deaths were similar. {$<$}br{$><$}br{$>$}CONCLUSIONS: Findings suggest widespread penetration of fentanyl in the drug supply, impacting a diverse group of users. An immediate scale-up of harm reduction and overdose prevention interventions is needed.{$<$}br{$><$}br{$>$}Copyright \textcopyright{} 2017 Elsevier B.V. All rights reserved.{$<$}p /{$>$}
{$<$}p{$>$}Language: en{$<$}/p{$>$}},
  copyright = {Elsevier Publishing},
  journal = {International journal on drug policy},
  language = {en}
}

